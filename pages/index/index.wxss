page{
  position: relative;
  background: #f0f0f0;
}
.com-widget-goTop { 
background-color: rgba(0,0,0,0.8); 
display: inline-block; 
width: 50px; 
height: 50px; 
line-height: 68px; 
font-size: 12px; 
color: #ffffff; 
text-align: center; 
border-radius: 50%; 
-webkit-background-size: 50px auto; 
}
.container{
  width: 100%;
  height: 100%;
  padding: 0;
}
.top{
  width: 100%;
  height: 40px;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: 1px solid #f0f0f0;
  padding-bottom: 3px;
  position: fixed;
  z-index: 3;
  background: white;
}
.top>view{
  display: flex;
  align-items: center;
  height: 36px;
  text-align: center;
  line-height: 36px;
}
.originRight{
  font-size: 15px;
  width: 71px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.search{
  background: #f0f0f0;
  height: 36px;
  border-radius: 5px;
  width: 258px;
  font-size: 15px;
  display: flex;
  align-items: center;
  margin-right: 5px;
}
.navigation{
  width: 100%;
  font-size: 15px;
  top: 40px;
  background: white;
  z-index: 2;
  height: 36px;
  position: fixed
  
}
.navigationPart{
  display: block;
  white-space: nowrap;
  padding-left: 15px;
  height: 36px;
  line-height: 36px;
   overflow:hidden;
}
.navigationPart view{
  height: 34px;
  display: inline-block;
  margin-right: 45px;
  line-height: 40px;
}
.navigationPart view:nth-last-child(1){
  margin-right: 60px;
}
.first{
  color: red;
  border-bottom: 2px solid red; 
} 
.arrow{
  position: fixed;
  background: white;
  right:0;
  top:43px;
  z-index: 2;
  width: 30px;
  height: 33px;
  display: flex;
 align-items: center;
}

.arrowDown{
  width: 8px ;
  height: 8px;
  border-bottom: 1px solid black;
  border-right: 1px solid black;
  transform: rotate(45deg);
  background: transparent;
  box-shadow:0px 0px 3px 4px white;
}
.navigationAll{
  background: white;
}
.kinds{
  width: 100%;
  background: white;
  display: flex;
  flex-wrap: wrap;
  font-size: 12px;
 z-index: 2;
  color: #464646;
  float: right;
}
.kinds view{
  flex-shrink: 0;
  border: 1px solid #f0f0f0;
  width: 70px;
  height: 25px;
  text-align: center;
  line-height: 25px;
  margin-left: 15px;
  margin-bottom: 15px;
}
 .kinds .kindsItem{
 border: 1px solid red; 
 color:red;
}

 .banner{
  width: 100%;
  height: 400rpx;
  z-index: 1;
  margin-top: 75px;
} 
swiper-item image{
  width: 100%;
}
.attention{
  width: 100%;
  height: 35px;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #ff6062;
  font-size: 24rpx;
  background: white;
  margin-left: 10rpx;
}
.attention>view{
  display: flex;
  align-items: center;
  height: 35px;
  width: 30%;
  margin-left: 30rpx
}
.attention>view>text{
  line-height: 16px;
}
/* .bannerImg{
  width: 20px;
  height: 20px;
  vertical-align: middle;
  margin-right: 5px;
} */
.attention>view .icon-sure{
  font-size: 16px;
  margin-right: 3px;
}
.shopKinds{
  width: 94%;
  display: block;
  padding-top: 10px;
  padding-bottom: 10px;
  white-space: nowrap;
  margin-left: 3%;
}
.nullView{
  width: 100%;
  height: 5px ;
  background: #f0f0f0;
}
