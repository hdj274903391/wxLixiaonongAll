.search{
  display: flex;
  justify-content: space-around;
  width: 100%;
  font-size: 15px;
  align-items: center;
  border-bottom: 1px solid #f0f0f0;
  height: 44px;
}
.searchInput{
  background: #f0f0f0;
  display: flex;
  width: 79%;
  border-radius: 5px;
  height: 33px;
  font-size: 14px;
  margin-left: 10px;
  position: relative;
}
.searchInput>text{
  line-height: 33px;
}
.origin{
  width: 20px;
  height: 20px;
  margin-left: 5px;
}
.inputContentList{
  width: 100%;
  position: fixed;
  top:45px; 
  display: flex;
  flex-direction: column;
  background: white;
  align-items: flex-end;
}
.input{
 color:#454545;height:33px;width:80%;
}
.inputContentItem{
  width: 95%;
  height: 45px;
  line-height: 45px;
  font-size: 15px;
  border-bottom: 1px solid #f0f0f0;
}
.inputContentItem>text{
  color: #cdcdcd;
}
.delate{
  position: absolute;
  right: 0;
  margin-right: 10px;
  line-height: 14px;
  float: right;
}
.history{
  width: 100%;
  padding-top: 6px;
}
.historyTitle{
  color: #969696;
  font-size: 14px;

  width: 100%;
  padding: 0 20px;
}
.historyList{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  padding:10px 20px 8px 20px;
}
.hisrotyItem{
  font-size: 12px;
  padding: 6px 12px;
  border: 1px solid #969696;
  margin-right: 20px;
  border-radius: 3px;
  color: #969696;
  flex-shrink: 0;
  margin-bottom: 10px;
  text-align: center;
  line-height: 15px;
}
.hot .hisrotyItem:nth-child(1),.hot .hisrotyItem:nth-child(2){
  color: #ff4c4d;
  border: 0.5px solid #ff4c4d;
}