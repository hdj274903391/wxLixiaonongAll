.container{
  width: 100%;
  height: 100vh;
  background: #f0f0f0;
  border-top: 1px solid #cdcdcd;
  position: absolute;
}
.infoList{
  width: 96%;
  color:#464646;
  padding-left: 4%;
  background: white;
  margin-top: 5px;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.infoItem{
  width: 100%;
  display: flex;
  height: 45px;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #f0f0f0;
  background: white;
  font-size: 13px;
  overflow: hidden;
}
.infoItem>view{
  margin-right: 4%;
}
.infoItem>input{
  margin-right: 4%;
  width: 200px;
  text-align: right;
}
.userPhoto{
  width: 55px;
  height: 55px;
  border-radius: 50%;
  background: #f0f0f0;
  margin-right: 15px;
}
.infoName{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100%;
  border-bottom: 1px solid #f0f0f0;
}
.arrow{
  width: 10px;
  height: 10px;
  border-right: 1px solid #cdcdcd;
  border-bottom: 1px solid #cdcdcd;
  transform: rotate(-45deg);
}
.keep{
  width: 93%;
  position: relative;
  bottom: 18px;
  background: linear-gradient(to right, #fe8657, #ff4c4d);
  color: white;
  height:40px;
  line-height: 40px;
  text-align: center;
  margin: 0 auto; 
  border-radius: 2px;
  font-size: 15px;
}
.picker{
    padding: 13px;
    background-color: #FFFFFF;
    width: 200px;
    text-align: end;
}
.radio-group {
  display: flex;
  height: 46px;
  overflow: hidden;
  align-items: center;
}
.radio {
    display: block;
    padding: 5px;
    transform: scale(0.8);
    font-size: 20px;
    line-height: 34px;
}
.defautSex{
  width: 300px;
  text-align: right;
}